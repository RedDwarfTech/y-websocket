{
  "name": "rdy-websocket",
  "version": "1.5.1",
  "description": "Websockets provider for Yjs",
  "main": "./dist/y-websocket.cjs",
  "module": "./src/y-websocket.ts",
  "types": "./dist/src/y-websocket.d.ts",
  "sideEffects": false,
  "funding": {
    "type": "GitHub Sponsors ‚ù§",
    "url": "https://github.com/sponsors/dmonad"
  },
  "scripts": {
    "start": "node ./bin/server-express.ts",
    "dist": "rm -rf dist && rollup -c && tsc",
    "build": "tsc --outDir dist/lib --declarationDir dist/types --declaration true && rollup -c",
    "vite-build": "vite build",
    "lint": "standard && tsc",
    "test": "npm run lint",
    "preversion": "npm run lint && npm run dist && test -e dist/src/y-websocket.d.ts && test -e dist/y-websocket.cjs"
  },
  "bin": {
    "y-websocket-server": "./bin/server.js",
    "y-websocket": "./bin/server.js"
  },
  "files": [
    "dist/*",
    "bin/*",
    "src/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./bin/utils": "./bin/utils.js",
    "./bin/callback": "./bin/callback.js",
    ".": {
      "module": "./src/y-websocket.ts",
      "import": "./src/y-websocket.js",
      "require": "./dist/y-websocket.cjs",
      "types": "./dist/src/y-websocket.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yjs/y-websocket.git"
  },
  "keywords": [
    "Yjs"
  ],
  "author": "Kevin Jahns <kevin.jahns@protonmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yjs/y-websocket/issues"
  },
  "homepage": "https://github.com/yjs/y-websocket#readme",
  "standard": {
    "ignore": [
      "/dist",
      "/node_modules"
    ]
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cnpm": "^9.2.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "lib0": "^0.2.52",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "log4js": "^6.9.1",
    "meilisearch": "^0.35.0",
    "prom-client": "^14.2.0",
    "prometheus-api-metrics": "^3.2.2",
    "vite": "^6.0.7",
    "y-protocols": "^1.0.5",
    "y-websocket": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.26.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/node": "^18.15.0",
    "rimraf": "^6.0.1",
    "rollup": "^3.27.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "standard": "^12.0.1",
    "tslib": "^2.8.1",
    "typescript": "^4.9.5",
    "yjs": "^13.5.0"
  },
  "peerDependencies": {
    "yjs": "^13.5.6"
  },
  "optionalDependencies": {
    "ws": "^6.2.1",
    "y-leveldb": "^0.1.0"
  }
}
